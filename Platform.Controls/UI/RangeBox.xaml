<core:InternalFlowComponent
    x:Class="Platform.Controls.UI.RangeBox"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:core="clr-namespace:Platform.Controls.Core"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:dhcc="http://www.dhcc.com.cn"
    xmlns:local="clr-namespace:Platform.Controls.UI"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:ui="clr-namespace:Platform.Controls.UI"
    x:Name="this"
    Title="跨度域"
    d:DesignHeight="30"
    d:DesignWidth="800"
    ScaleSize="2,1"
    mc:Ignorable="d">
    <StackPanel x:Name="hostPanel" Orientation="Horizontal">
        <ContentPresenter Content="{Binding ElementName=this, Path=RangeStart}" />
        <dhcc:IconBox
            Width="25"
            Margin="10,5"
            Code="&#xf061;"
            Foreground="Silver" />
        <ContentPresenter Content="{Binding ElementName=this, Path=RangeEnd}" />
    </StackPanel>
</core:InternalFlowComponent>
